<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="inti.SAhomepage.article.repository.FileMapper">
    <select id="getArticleFileNames" resultType="string">
        SELECT file_name
        FROM tbl_article_file
        WHERE article_no = #{articleNo}
    </select>

    <select id="getArticleFileId" resultType="int">
        SELECT id
        FROM tbl_article_file
        WHERE article_no = #{articleNo}
    </select>

</mapper>